Eligible for Placement

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char ch[20];
    int a;
    int cg;
    scanf("%s",ch);
    scanf("%d",&a);
    scanf("%d",&cg);
    if(a==1 && cg>69)
    {
    printf("Name of the Student:%s\n",ch);
    printf("%s is Eligible for Placement",ch);
    }
    else if(1<=a && cg>74)
    {
    printf("Name of the Student:%s\n",ch);
    printf("%s is  Eligible for Placement",ch);
    }
    else
    {
    printf("Name of the Student:%s\n",ch);
    printf("%s is Not Eligible for Placement",ch);
    }
    return 0;
}
