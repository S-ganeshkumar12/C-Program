Delete the blank space

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>
#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    char str[1000], result[1000];
    int i = 0, j = 0;
    scanf("%[^\n]",str);
    while (str[i] != '\0') {
        if (str[i] != ' ' && str[i] != '\n') {
            result[j++] = str[i];
        }
        i++;
    }
    result[j] = '\0'; 
    printf("%s\n", result);
    return 0;
}
