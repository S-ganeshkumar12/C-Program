2D Prime Matrix

#include <stdio.h>
#include <stdbool.h>
#include <math.h>

bool isPrime(int num) {
    if (num < 2) return false;
    for (int i = 2; i <= sqrt(num); i++) {
        if (num % i == 0)
            return false;
    }
    return true;
}

int main() {
    int m;
    scanf("%d", &m);

    if (m > 6 || m <= 0) {
        printf("Enter the valid Input\n");
        return 0;
    }

    int matrix[m][m];
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < m; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }

    printf("The Prime Array Matrix is:\n");
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < m; j++) {
            if (isPrime(matrix[i][j])) {
                printf("F ");
            } else {
                printf("T ");
            }
        }
        printf("\n");
    }

    return 0;
}
