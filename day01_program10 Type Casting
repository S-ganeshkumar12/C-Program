Type Casting

  #include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int a;
    scanf("%d\n",&a);

    char b;
    scanf("%c",&b);
    int x=(int)b;
    printf("Integer of %d is %c\n",a,(char)a);
    printf("Character of %c is %d",b,x);    
    return 0;
}
